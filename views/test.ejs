<!DOCTYPE html>
<html lang="ko" ng-app="bikeda">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Test</title>

  <!-- Bootstrap core CSS -->
  <!-- <link href="css/bootstrap.min.css" rel="stylesheet"> -->
  <link href="css/bootstrap.css" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="css/bikeda.css" rel="stylesheet">
  <link href="css/bikeda_util.css" rel="stylesheet">

  <!-- Bootstrap core JavaScript -->
  <script src="js/jquery.min.js"></script>
  <script src="js/bootstrap.bundle.min.js"></script>
  <script src="socket.io-client/dist/socket.io.js"></script>
  <script src="angular/angular.js"></script>
  <script src="js/bikeda.js"></script>
</head>
<body >
<div class="limiter" >
  <div class="container" ng-controller="loginCtrl">
    <div class="wrap p-l-55 p-r-55 p-t-65 p-b-50">
      <form class="login-form validate-form" name="loginFrom" ng-submit="login()">
        <span class="login-form-title p-b-33">
          TEST PAGE
        </span>
        <div class="wrap-input validate-input" data-validate="아이디를 입력하세요">
          <input class="input" type="text" name="id" placeholder="아이디" ng-model="id">
          <span class="focus-input-1"></span>
          <span class="focus-input-2"></span>
        </div>
        <div class="wrap-input rs1 validate-input" data-validate="비밀번호를 입력하세요.">
          <input class="input" type="password" name="pass" placeholder="비밀번호" ng-model="pwd">
          <span class="focus-input-1"></span>
          <span class="focus-input-2"></span>
        </div>
        <div class="container-form-btn m-t-20">
          <button class="login-form-btn" >
            로그인
          </button>
        </div>
      </form>
    </div>

    <li ng-repeat="login in logins">
      <span>{{login.id}} / {{login.pwd}}</span>
    </li>
  </div>

</div>

</body>
</html>
